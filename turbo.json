{
  "$schema": "https://turborepo.com/schema.json",
  "globalDependencies": [
    "tsconfig.json",
    "tsconfig.base.json",
    ".env"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        "lib/dom/build/**",
        "lib/v3/dom/build/**",
        "lib/version.ts",
        "openapi.v3.yaml"
      ],
      "inputs": [
        "**/*.ts",
        "**/*.js",
        "package.json",
        "tsconfig.json",
        "tsconfig.*.json"
      ]
    },
    "build:esm": {
      "dependsOn": ["^build:esm"],
      "outputs": [
        "dist/esm/**",
        "lib/dom/build/**",
        "lib/v3/dom/build/**",
        "lib/version.ts"
      ],
      "inputs": [
        "**/*.ts",
        "**/*.js",
        "package.json",
        "tsconfig.json",
        "tsconfig.*.json"
      ]
    },
    "build:esm-tests": {
      "dependsOn": ["^build:esm"],
      "outputs": ["dist/tests/**"],
      "inputs": [
        "test/**/*.ts",
        "package.json",
        "tsconfig.json",
        "tsconfig.*.json"
      ]
    },
    "build:sea:esm": {
      "dependsOn": ["^build:esm"],
      "outputs": [
        "dist/sea/**",
        "dist/app.mjs"
      ],
      "inputs": [
        "src/**/*.ts",
        "scripts/**/*.ts",
        "package.json",
        "tsconfig.json",
        "tsconfig.*.json"
      ]
    },
    "build:sea:cjs": {
      "dependsOn": ["^build:cjs"],
      "outputs": ["dist/sea/**"],
      "inputs": [
        "src/**/*.ts",
        "scripts/**/*.ts",
        "package.json",
        "tsconfig.json",
        "tsconfig.*.json"
      ]
    },
    "build:cjs": {
      "dependsOn": ["^build:cjs"],
      "outputs": [
        "dist/cjs/**",
        "lib/dom/build/**",
        "lib/v3/dom/build/**",
        "lib/version.ts"
      ],
      "inputs": [
        "**/*.ts",
        "**/*.js",
        "package.json",
        "tsconfig.json",
        "tsconfig.*.json"
      ]
    },
    "build:cli": {
      "dependsOn": ["^build"],
      "outputs": ["dist/cli/**"],
      "inputs": [
        "cli.ts",
        "evals.config.json",
        "scripts/**"
      ]
    },
    "lint": {
      "outputs": [],
      "inputs": [
        "**/*.ts",
        "**/*.js",
        "**/*.jsx",
        ".eslintrc*",
        ".prettierrc*"
      ]
    },
    "format": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": [
        "**/*.spec.ts",
        "**/*.test.ts"
      ]
    },
    "test:core": {
      "dependsOn": ["build:esm"],
      "outputs": [],
      "cache": false,
      "inputs": [
        "tests/**/*.ts",
        "package.json",
        "tsconfig.json",
        "tsconfig.*.json"
      ]
    },
    "test:e2e": {
      "dependsOn": ["build:esm"],
      "outputs": [
        "playwright-report/**",
        "test-results/**"
      ],
      "cache": false,
      "inputs": [
        "lib/v3/tests/**/*.ts",
        "package.json",
        "tsconfig.json",
        "tsconfig.*.json"
      ]
    },
    "test:evals": {
      "dependsOn": ["build:esm", "build:cli"],
      "outputs": [],
      "cache": false,
      "inputs": [
        "scripts/**",
        "cli.ts",
        "evals.config.json",
        "package.json",
        "tsconfig.json",
        "tsconfig.*.json"
      ]
    },
    "test:server": {
      "dependsOn": ["build:sea:esm", "build:esm-tests"],
      "outputs": [],
      "cache": false,
      "inputs": [
        "test/**/*.ts",
        "scripts/**",
        "src/**/*.ts",
        "package.json",
        "tsconfig.json",
        "tsconfig.*.json"
      ]
    },
    "e2e": {
      "dependsOn": ["build"],
      "outputs": [
        "playwright-report/**",
        "test-results/**"
      ],
      "cache": false
    },
    "e2e:local": {
      "dependsOn": ["build"],
      "outputs": [
        "playwright-report/**",
        "test-results/**"
      ],
      "cache": false,
      "env": [
        "BROWSERBASE_API_KEY",
        "BROWSERBASE_PROJECT_ID",
        "CHROME_PATH",
        "CTRF_JUNIT_PATH",
        "GOOGLE_GENERATIVE_AI_API_KEY",
        "LOCAL_SESSION_LIMIT_PER_E2E_TEST",
        "NODE_V8_COVERAGE",
        "ANTHROPIC_API_KEY",
        "OPENAI_API_KEY"
      ]
    },
    "e2e:bb": {
      "dependsOn": ["build"],
      "outputs": [
        "playwright-report/**",
        "test-results/**"
      ],
      "cache": false,
      "env": [
        "BROWSERBASE_REGION",
        "BROWSERBASE_SESSION_LIMIT_PER_E2E_TEST",
        "CHROME_PATH",
        "CTRF_JUNIT_PATH",
        "NODE_V8_COVERAGE",
        "BROWSERBASE_API_KEY",
        "BROWSERBASE_PROJECT_ID",
        "OPENAI_API_KEY",
        "ANTHROPIC_API_KEY",
        "GOOGLE_GENERATIVE_AI_API_KEY"
      ]
    },
    "evals": {
      "dependsOn": ["build"],
      "outputs": [],
      "cache": false,
      "env": [
        "BROWSERBASE_API_KEY",
        "BROWSERBASE_PROJECT_ID",
        "OPENAI_API_KEY",
        "ANTHROPIC_API_KEY",
        "GOOGLE_GENERATIVE_AI_API_KEY",
        "BRAINTRUST_API_KEY"
      ]
    },
    "docs": {
      "persistent": true,
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
