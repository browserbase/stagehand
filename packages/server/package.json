{
  "name": "@browserbasehq/stagehand-server",
  "version": "3.0.5",
  "description": "Self-hostable Fastify server for Stagehand",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --outDir dist",
    "typecheck": "tsc --noEmit",
    "lint": "cd ../.. && prettier --check packages/server && cd packages/server && eslint .",
    "format": "prettier --write .",
    "dev": "tsx src/dev.ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "stagehand",
    "fastify",
    "server"
  ],
  "author": "Browserbase",
  "license": "MIT",
  "dependencies": {
    "@browserbasehq/stagehand": "workspace:*",
    "@fastify/cors": "^10.0.1",
    "fastify": "^5.2.4",
    "fastify-type-provider-zod": "^6.1.0",
    "pino-pretty": "^13.0.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "eslint": "^9.16.0",
    "prettier": "^3.2.5",
    "tsup": "^8.2.1",
    "tsx": "^4.10.5",
    "typescript": "^5.2.2"
  }
}
