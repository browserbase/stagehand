{
  "name": "@browserbasehq/stagehand-server",
  "version": "3.0.5",
  "description": "Stagehand API server",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsx watch src/server.ts",
    "lint": "cd ../.. && prettier --check packages/server && cd packages/server && eslint .",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org browserbase --project stagehand-api ./dist && sentry-cli sourcemaps upload --org browserbase --project stagehand-api ./dist",
    "node:test": "node --import tsx --test --test-reporter=spec",
    "test:unit": "pnpm run node:test $(find ./src -type f -name '*.test.ts')",
    "test:unit:local": "DOTENV_CONFIG_PATH=.env.local pnpm run test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@browserbasehq/sdk": "^2.4.0",
    "@browserbasehq/stagehand": "workspace:*",
    "@browserbasehq/zod": "workspace:*",
    "@fastify/cors": "^11.0.1",
    "@sentry/node": "8.35.0",
    "@t3-oss/env-nextjs": "^0.11.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.30.4",
    "drizzle-orm": "^0.38.4",
    "fastify": "^5.3.2",
    "fastify-metrics": "^12.1.0",
    "fastify-plugin": "^4.5.1",
    "http-status-codes": "^2.3.0",
    "pino": "^9.7.0",
    "pino-pretty": "^11.3.0",
    "playwright": "1.52.0",
    "postgres": "^3.4.5",
    "stagehand-v3": "npm:@browserbasehq/orca@3.0.3-patch",
    "uuid": "^11.0.5",
    "zod": "^3.25.0",
    "zod-to-json-schema": "^3.24.3"
  },
  "devDependencies": {
    "@sentry/cli": "^2.41.1",
    "@types/node": "22.13.1",
    "eslint": "^9.7.0",
    "openai": "4.87.1",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.18.0",
    "typescript": "~5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/browserbase/stagehand.git",
    "directory": "packages/server"
  },
  "bugs": {
    "url": "https://github.com/browserbase/stagehand/issues"
  },
  "homepage": "https://stagehand.dev"
}
