{
  "name": "@browserbasehq/stagehand",
  "version": "3.0.8",
  "description": "An AI web browsing framework focused on simplicity and extensibility.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "gen-version": "tsx scripts/gen-version.ts",
    "build-dom-scripts": "tsx lib/v3/dom/genDomScripts.ts && tsx lib/v3/dom/genLocatorScripts.ts && tsx lib/v3/dom/genA11yScripts.ts",
    "build-js": "tsup --entry.index lib/v3/index.ts --dts --sourcemap",
    "build:esm": "tsc -p tsconfig.esm.json && pnpm exec esbuild \"tests/**/*.ts\" \"lib/v3/tests/**/*.ts\" --outdir=dist/esm --outbase=. --format=esm --platform=node --sourcemap --log-level=warning && node -e \"const fs=require('node:fs');const path=require('node:path');const out=path.join('dist','esm');fs.mkdirSync(out,{recursive:true});fs.writeFileSync(path.join(out,'package.json'), '{\\n  \\\"type\\\": \\\"module\\\"\\n}\\n');const wrapper=\\\"import * as Stagehand from './lib/v3/index.js';\\\\nexport * from './lib/v3/index.js';\\\\nexport default Stagehand;\\\\n\\\";fs.writeFileSync(path.join(out,'index.js'), wrapper);const srcBuild=path.join('lib','v3','dom','build');const destBuild=path.join(out,'lib','v3','dom','build');fs.mkdirSync(destBuild,{recursive:true});for (const file of fs.readdirSync(srcBuild)) {if (file.endsWith('.js')) {fs.copyFileSync(path.join(srcBuild,file), path.join(destBuild,file));}}\"",
    "typecheck": "tsc --noEmit",
    "build": "pnpm run gen-version && pnpm run build-dom-scripts && pnpm run build-js && pnpm run typecheck",
    "example": "node --import tsx -e \"const args=process.argv.slice(1).filter(a=>a!=='--'); const [p]=args; const n=(p||'example').replace(/^\\.\\//,'').replace(/\\.ts$/i,''); import(new URL(require('node:path').resolve('examples', n + '.ts'), 'file:'));\" --",
    "test": "NODE_OPTIONS='--import tsx --import ../../scripts/register-stagehand-dist.mjs' playwright test --config=lib/v3/tests/v3.playwright.config.ts",
    "test:esm": "NODE_OPTIONS='--enable-source-maps --experimental-specifier-resolution=node' playwright test --config=dist/esm/lib/v3/tests/v3.playwright.config.js",
    "e2e": "NODE_OPTIONS='--import tsx --import ../../scripts/register-stagehand-dist.mjs' playwright test --config=lib/v3/tests/v3.local.playwright.config.ts",
    "e2e:local": "NODE_OPTIONS='--import tsx --import ../../scripts/register-stagehand-dist.mjs' playwright test --config=lib/v3/tests/v3.local.playwright.config.ts",
    "e2e:local:esm": "NODE_OPTIONS='--enable-source-maps --experimental-specifier-resolution=node' playwright test --config=dist/esm/lib/v3/tests/v3.local.playwright.config.js",
    "e2e:bb": "NODE_OPTIONS='--import tsx --import ../../scripts/register-stagehand-dist.mjs' playwright test --config=lib/v3/tests/v3.bb.playwright.config.ts",
    "e2e:bb:esm": "NODE_OPTIONS='--enable-source-maps --experimental-specifier-resolution=node' playwright test --config=dist/esm/lib/v3/tests/v3.bb.playwright.config.js",
    "lint": "cd ../.. && prettier --check packages/core && cd packages/core && eslint .",
    "format": "prettier --write .",
    "test:vitest": "turbo run build && vitest run --config vitest.config.ts",
    "test:vitest:esm": "vitest run --config vitest.esm.config.mjs"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/lib",
    "dist/types",
    "dist/stagehand.config.d.ts"
  ],
  "keywords": [
    "ai",
    "browser",
    "automation",
    "web-scraping",
    "testing"
  ],
  "author": "Browserbase",
  "license": "MIT",
  "peerDependencies": {
    "deepmerge": "^4.3.1",
    "dotenv": "^16.4.5",
    "zod": "^3.25.76 || ^4.2.0"
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "uuid": "^11.1.0",
    "@anthropic-ai/sdk": "0.39.0",
    "@browserbasehq/sdk": "^2.4.0",
    "@google/genai": "^1.22.0",
    "@langchain/openai": "^0.4.4",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "ai": "^5.0.0",
    "devtools-protocol": "^0.0.1464554",
    "fetch-cookie": "^3.1.0",
    "openai": "^4.87.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "ws": "^8.18.0",
    "zod-to-json-schema": "^3.25.0"
  },
  "optionalDependencies": {
    "@ai-sdk/anthropic": "^2.0.34",
    "@ai-sdk/azure": "^2.0.54",
    "@ai-sdk/cerebras": "^1.0.25",
    "@ai-sdk/deepseek": "^1.0.23",
    "@ai-sdk/google": "^2.0.23",
    "@ai-sdk/google-vertex": "^3.0.70",
    "@ai-sdk/groq": "^2.0.24",
    "@ai-sdk/mistral": "^2.0.19",
    "@ai-sdk/openai": "^2.0.53",
    "@ai-sdk/perplexity": "^2.0.13",
    "@ai-sdk/togetherai": "^1.0.23",
    "@ai-sdk/xai": "^2.0.26",
    "@langchain/core": "^0.3.40",
    "bufferutil": "^4.0.9",
    "chrome-launcher": "^1.2.0",
    "ollama-ai-provider-v2": "^1.5.0",
    "patchright-core": "^1.55.2",
    "playwright": "^1.52.0",
    "playwright-core": "^1.54.1",
    "puppeteer-core": "^22.8.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.8",
    "@playwright/test": "^1.42.1",
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^20.11.30",
    "@types/ws": "^8.5.13",
    "adm-zip": "^0.5.16",
    "chalk": "^5.4.1",
    "esbuild": "^0.21.4",
    "eslint": "^9.16.0",
    "playwright": "^1.52.0",
    "playwright-core": "^1.54.1",
    "prettier": "^3.2.5",
    "tsup": "^8.2.1",
    "tsx": "^4.10.5",
    "typescript": "^5.2.2",
    "vitest": "^4.0.8",
    "zod": "^3.25.76 || ^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/browserbase/stagehand.git",
    "directory": "packages/core"
  },
  "bugs": {
