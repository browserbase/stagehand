{
  "all": true,
  "include": [
    "lib/v3/**/*.ts",
    "!lib/v3/**/*.spec.ts",
    "!lib/v3/**/*.d.ts"
  ],
  "exclude": [
    "**/*.spec.ts",
    "**/tests/**",
    "**/node_modules/**",
    "**/dist/**"
  ],
  "extension": [".ts"],
  "reporter": [
    "html",
    "text",
    "text-summary",
    "lcov"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./.nyc_output",
  "instrument": true,
  "sourceMap": true,
  "produce-source-map": true,
  "lines": 50,
  "functions": 50,
  "branches": 50,
  "statements": 50
}
