{
  "name": "@browserbasehq/stagehand-evals",
  "version": "1.1.7",
  "private": true,
  "description": "Evaluation suite for Stagehand",
  "type": "module",
  "main": "./",
  "scripts": {
    "typecheck": "pnpm -w --dir ../.. exec tsc -p packages/evals/tsconfig.json --noEmit",
    "build": "pnpm run build:esm && pnpm run build:cli && pnpm run typecheck",
    "build:esm": "tsx scripts/build-esm.ts",
    "build:cli": "tsx scripts/build-cli.ts",
    "test:evals": "tsx scripts/test-evals.ts",
    "evals": "pnpm run test:evals --",
    "lint": "cd ../.. && prettier --check packages/evals && cd packages/evals && eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@browserbasehq/stagehand": "workspace:*",
    "ai": "^5.0.0",
    "dotenv": "16.4.5",
    "openai": "^4.87.1",
    "sharp": "^0.34.5",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "braintrust": "^0.4.7",
    "chalk": "^5.4.1",
    "string-comparison": "^1.3.0",
    "tsx": "*"
  }
}
